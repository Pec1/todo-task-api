generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Priority {
  LOW
  MEDIUM
  HIGH
}

model Task {
  id          String    @id @default(uuid())
  title       String
  description String
  priority    Priority?
  completed   Boolean   @default(false)
  createdAT   DateTime  @default(now())
  endAT       DateTime?
}
